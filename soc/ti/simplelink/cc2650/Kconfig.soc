config SOC_SERIES_CC2650
	bool
	select SOC_FAMILY_TI_SIMPLELINK
	help
	  Enable support for TI SimpleLink CC2650 SoCs

config SOC_CC2650
	bool
	select SOC_SERIES_CC2650
	help
	  CC2650

config SOC_SERIES
	default "cc2650" if SOC_SERIES_CC2650

config SOC
	default "cc2650" if SOC_CC2650
